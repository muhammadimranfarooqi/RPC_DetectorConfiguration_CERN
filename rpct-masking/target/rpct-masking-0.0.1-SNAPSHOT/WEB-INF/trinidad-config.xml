<?xml version="1.0" encoding="UTF-8"?>
<trinidad-config xmlns="http://myfaces.apache.org/trinidad/config">
	<debug-output>true</debug-output>
	<!--
		Uncomment to switch back to ALERT style client-side validation, or set
		to DISABLED to disable it altogether
		<client-validation>ALERT</client-validation>
	-->

	<accessibility-mode>default</accessibility-mode>
	<accessibility-profile>default</accessibility-profile>

	<!--
		you can use EL to get the skin. This allows the skin to change between
		requests.
	-->

	<skin-family>simple</skin-family>
</trinidad-config>